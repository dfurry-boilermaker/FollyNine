{
  "version": 2,
  "builds": [
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/public/$1" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "framework": "static",
  "regions": ["iad1"],
  "env": {
    "NODE_VERSION": "18"
  }
} 